<body>
    <div class="main-wrapper">
        <div class="header">
			<div class="header-left">
				<a class="logo">
					<img src="/stylesheets/assets/img/logo.png" width="35" height="35" alt=""> <span>Preclinic</span>
				</a>
			</div>
			<a id="toggle_btn" href="javascript:void(0);"><i class="fa fa-bars"></i></a>
            <a id="mobile_btn" class="mobile_btn float-left" href="#sidebar"><i class="fa fa-bars"></i></a>
            <ul class="nav user-menu float-right">

                <li class="nav-item dropdown has-arrow">
                    <a href="#" class="dropdown-toggle nav-link user-link" data-toggle="dropdown">
                        <span class="user-img"><img class="rounded-circle" src="/stylesheets/assets/img/user.jpg" width="40" alt="Admin">
							</span>
                        <span>Doctor</span>
                    </a>
                    <div class="dropdown-menu">
				
        <div class="dropdown-menu">
            
            <a class="dropdown-item" href="/doctor/logout">Logout</a>
        </div>
    </li>
</ul>
<div class="dropdown mobile-user-menu float-right">
    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
    <div class="dropdown-menu dropdown-menu-right">
       
        <a class="dropdown-item" href="/doctor/logout">Logout</a>
    </div>
</div>
</div>
        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li class="menu-title">Main</li>
                        <li>
                            <a href="/dashboard"><i class="fa fa-dashboard"></i> <span>Dashboard</span></a>
                        </li>
                        <li >
                            <a href="/allPatients"><i class="fa fa-hospital-o"></i> <span>All Patients</span></a>
                        </li>
                        <li >
                            <a href="/patients"><i class="fa fa-wheelchair"></i> <span>My Patients</span></a>
                        </li>
                        <li class="active">
                            <a href="/allDoctors"><i class="fa fa-user-md"></i> <span>Doctors</span></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-sm-4 col-3">
                        <h4 class="page-title">Doctors</h4>
                    </div>
                </div>
				<div class="row">
					<div class="col-md-12">
						<div class="table-responsive">
							<table class="table table-border table-striped custom-table mb-0">
								<thead>
									<tr>
										<th>Doctor Name</th>
										<th>Email</th>
                                        <th>Phone Number</th>
										<th>specialization</th>
									</tr>
								</thead>
								<tbody>
                                    <% doctors.forEach((doctor) => { %>
									<tr>
										<td><img width="28" height="28" src="<%=doctor.image%>" class="rounded-circle m-r-5" alt=""> <%=doctor.DoctorsName%></td>
										<td><%=doctor.email%></td>
										<td><%=doctor.phoneNumber%></td>
										<td><%=doctor.specialization%></td>
									</tr>
                                    <%})%>
								</tbody>
							</table>
						</div>
                </div>
                </div>
            </div>
            
        </div>
		
    </div>
    <div class="sidebar-overlay" data-reff=""></div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js" integrity="sha512-pumBsjNRGGqkPzKHndZMaAG+bir374sORyzM3uulLV14lN5LyykqNk8eEeUlUkB3U0M4FApyaHraT65ihJhDpQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
