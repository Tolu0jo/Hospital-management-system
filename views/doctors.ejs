    <script src="/stylesheets/assets/js/moment.min.js"></script>
    <script src="/stylesheets/assets/js/bootstrap-datetimepicker.min.js"></script>
    <script src="/stylesheets/assets/js/app.js"></script>
    <script src="/stylesheets/https://cdn.datatables.net/v/bs4/dt-1.13.4/datatables.min.js"></script>
    <script>
        $(document).ready(function(){
            $("table").dataTable({
                order:[0,'desc']
            });
        });
    </script>
</body>


