<!DOCTYPE html>
<html lang="en">


<!-- patients23:17-->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <link rel="shortcut icon" type="image/x-icon" href="/stylesheets/assets/img/favicon.ico">
    <title>Preclinic - Medical & Hospital </title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/assets/css/style.css">
    <!--[if lt IE 9]>
		<script src="/stylesheets/assets/js/html5shiv.min.js"></script>
		<script src="/stylesheets/assets/js/respond.min.js"></script>
	<![endif]-->
</head>

<body>
    <div class="main-wrapper">
        <div class="header">
			<div class="header-left">
				<a class="logo">
					<img src="/stylesheets/assets/img/logo.png" width="35" height="35" alt=""> <span>Preclinic</span>
				</a>
			</div>
			<a id="toggle_btn" href="javascript:void(0);"><i class="fa fa-bars"></i></a>
            <a id="mobile_btn" class="mobile_btn float-left" href="#sidebar"><i class="fa fa-bars"></i></a>
            <ul class="nav user-menu float-right">
               
               
                <li class="nav-item dropdown has-arrow">
                    <a href="#" class="dropdown-toggle nav-link user-link" data-toggle="dropdown">
                        <span class="user-img"><img class="rounded-circle" src="/stylesheets/assets/img/user.jpg" width="40" alt="Admin">
							</span>
                        <span>Doctor</span>
                    </a>
					<div class="dropdown-menu">
						<a class="dropdown-item" href="/doctor/logout">Logout</a>
					</div>
                </li>
            </ul>
            <div class="dropdown mobile-user-menu float-right">
                <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                <div class="dropdown-menu dropdown-menu-right"> 
                    <a class="dropdown-item" href="/doctor/logout">Logout</a>
                </div>
            </div>
        </div>
        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li class="menu-title">Main</li>
                        <li>
                            <a href="/dashboard"><i class="fa fa-dashboard"></i> <span>Dashboard</span></a>
                        </li>
                        <li>
                            <a href="/allPatients"><i class="fa fa-hospital-o"></i> <span>All Patients</span></a>
                        </li>
                        <li class="active">
                            <a href="/patients"><i class="fa fa-wheelchair"></i> <span>My Patients</span></a>
                        </li> 
                        <li >
                            <a href="/allDoctors"><i class="fa fa-user-md"></i> <span>Doctors</span></a>
                        </li>
                </div>
            </div>
        </div>
        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-sm-4 col-3">
                        <h4 class="page-title">Patients</h4>
                    </div>
                    <div class="col-sm-8 col-9 text-right m-b-20">
                        <a href="/createpatient" class="btn btn btn-primary btn-rounded float-right"><i class="fa fa-plus"></i> Add Patient</a>
                    </div>
                </div>
				<div class="row">
					<div class="col-md-12">
						<div class="table-responsive">
							<table class="table table-border table-striped custom-table datatable mb-0">
								<thead>
									<tr>
										<th>Name</th>
										<th>Age</th>
                                        <th>Weight</th>
                                        <th>Height</th>
										<th>Blood Group</th>
										<th>Genotype</th>
										<th>Blood Pressure</th>
                                        <th>Hiv Status</th>
                                        <th>Hepatitis</th>
                                      
                                        
                                        <!-- <th>Status</th> -->
										<th class="text-right">Action</th>
									</tr>
								</thead>
								<tbody>
                                    <% patients.forEach((patient) => { %>
                                        <tr>
                                            <td><img width="28" height="28" src="/stylesheets/assets/img/user.jpg" class="rounded-circle m-r-5" alt=""> <%=patient.patientName%></td>
                                            <td><%=patient.age%></td>
                                            <td><%=patient.weight%></td>
                                            <td><%=patient.height%></td>
                                            <td><%=patient.bloodGroup%></td>
                                            <td><%=patient.genotype%></td>
                                            <td><%=patient.bloodPressure%></td>
                                            <td><%=patient.HIV_status%></td>
                                            <td><%=patient.hepatitis%></td>

                                            <td class="text-right">
                                                <div class="dropdown dropdown-action">
                                                    <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                                    <div class="dropdown-menu dropdown-menu-right">
                                                        <a class="dropdown-item" href="patient/editPage/<%=patient._id%>"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                        <a class="dropdown-item" href="/patient/deletePatient/<%=patient._id%>" ><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                        <% }) %>
                                </tbody>
							</table>
						</div>
					</div>
                </div>
            </div>
        </div>

    </div>
    <div class="sidebar-overlay" data-reff=""></div>
    <script src="/stylesheets/assets/js/jquery-3.2.1.min.js"></script>
	<script src="/stylesheets/assets/js/popper.min.js"></script>
    <script src="/stylesheets/assets/js/bootstrap.min.js"></script>
    <script src="/stylesheets/assets/js/jquery.slimscroll.js"></script>
    <script src="/stylesheets/assets/js/select2.min.js"></script>
    <script src="/stylesheets/assets/js/jquery.dataTables.min.js"></script>
    <script src="/stylesheets/assets/js/dataTables.bootstrap4.min.js"></script>
    <script src="/stylesheets/assets/js/moment.min.js"></script>
    <script src="/stylesheets/assets/js/bootstrap-datetimepicker.min.js"></script>
    <script src="/stylesheets/assets/js/app.js"></script>
</body>


<!-- patients23:19-->
</html>